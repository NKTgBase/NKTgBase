! Simula: early object-oriented language
Begin
  Real x, v, m, dm_dt, p, NKTg1, NKTg2;
  x := 2.0; v := 3.0; m := 5.0; dm_dt := 0.1;
  p := m * v;
  NKTg1 := x * p;
  NKTg2 := dm_dt * p;
  OutText("p="); OutReal(p, 2); OutText(" NKTg1="); OutReal(NKTg1, 2); OutText(" NKTg2="); OutReal(NKTg2, 2); OutImage;
End;
